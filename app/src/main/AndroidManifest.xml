<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nusupper">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logocircle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/nusupper_logo_no_text"
        android:supportsRtl="true"
        android:theme="@style/Theme.NUSupper"
        tools:targetApi="31">
        <activity
            android:name=".ViewJio"
            android:exported="true" />
        <activity
            android:name=".LaunchLogo"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FindJio"
            android:exported="false" />
        <activity
            android:name=".Template"
            android:exported="false" />
        <activity
            android:name=".CreateJio"
            android:exported="false" /> <!-- maybe change this back to false later -->
        <activity
            android:name=".Profile"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>